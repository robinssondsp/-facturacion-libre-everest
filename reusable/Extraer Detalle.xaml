              <ui:InvokeCode ContinueOnError="{x:Null}" Code="' Argumento requerido (Edit Arguments):&#xA;' dtVertical  In/Out  System.Data.DataTable&#xA;&#xA;If dtVertical Is Nothing Then&#xA;    Return&#xA;End If&#xA;&#xA;If dtVertical.Rows.Count = 0 Then&#xA;    Return&#xA;End If&#xA;&#xA;If Not dtVertical.Columns.Contains(&quot;Column0&quot;) Then&#xA;    Return&#xA;End If&#xA;&#xA;Dim raw As String = &quot;&quot;&#xA;If dtVertical.Rows(0)(&quot;Column0&quot;) IsNot Nothing Then&#xA;    raw = dtVertical.Rows(0)(&quot;Column0&quot;).ToString()&#xA;End If&#xA;&#xA;raw = raw.Replace(vbCrLf, vbLf).Replace(vbCr, vbLf)&#xA;&#xA;Dim arr() As String = raw.Split(New String() {vbLf}, System.StringSplitOptions.None)&#xA;&#xA;Dim dtOut As New System.Data.DataTable()&#xA;dtOut.Columns.Add(&quot;Column0&quot;, GetType(String))  ' Nombre&#xA;dtOut.Columns.Add(&quot;Column1&quot;, GetType(String))  ' SKU&#xA;dtOut.Columns.Add(&quot;Column2&quot;, GetType(String))  ' Precio (sin separador de miles &quot;.&quot;)&#xA;dtOut.Columns.Add(&quot;Column3&quot;, GetType(Integer)) ' Cantidad&#xA;&#xA;Dim currentName As String = &quot;&quot;&#xA;Dim currentSku As String = &quot;&quot;&#xA;Dim currentPrice As String = &quot;&quot;&#xA;Dim currentQty As Integer = 0&#xA;&#xA;' SKU puede venir en una línea que NO empieza con &quot;SKU&quot; (ej: &quot;Color: RGB | SKU 80015 X2&quot;)&#xA;Dim rxSku As New System.Text.RegularExpressions.Regex(&#xA;    &quot;SKU\s*([A-Z0-9 ]+)&quot;,&#xA;    System.Text.RegularExpressions.RegexOptions.IgnoreCase&#xA;)&#xA;&#xA;Dim rxPrice As New System.Text.RegularExpressions.Regex(&quot;\$\s*([0-9\.\,]+)&quot;, System.Text.RegularExpressions.RegexOptions.IgnoreCase)&#xA;Dim rxQty As New System.Text.RegularExpressions.Regex(&quot;^\s*(\d+)\s*u\.\s*$&quot;, System.Text.RegularExpressions.RegexOptions.IgnoreCase)&#xA;&#xA;Dim rxIgnore As New System.Text.RegularExpressions.Regex(&#xA;    &quot;^(Venta\s*#|Seleccionar\s+archivos|Seleccionar\s+o\s+arrastrar)&quot;,&#xA;    System.Text.RegularExpressions.RegexOptions.IgnoreCase&#xA;)&#xA;&#xA;Dim flushAction As System.Action =&#xA;    Sub()&#xA;        If Not String.IsNullOrWhiteSpace(currentName) AndAlso&#xA;           Not String.IsNullOrWhiteSpace(currentSku) AndAlso&#xA;           Not String.IsNullOrWhiteSpace(currentPrice) AndAlso&#xA;           currentQty &gt; 0 Then&#xA;&#xA;            Dim r As System.Data.DataRow = dtOut.NewRow()&#xA;            r(&quot;Column0&quot;) = currentName.Trim()&#xA;            r(&quot;Column1&quot;) = currentSku.Trim()&#xA;            r(&quot;Column2&quot;) = currentPrice.Trim()&#xA;            r(&quot;Column3&quot;) = currentQty&#xA;            dtOut.Rows.Add(r)&#xA;        End If&#xA;&#xA;        currentName = &quot;&quot;&#xA;        currentSku = &quot;&quot;&#xA;        currentPrice = &quot;&quot;&#xA;        currentQty = 0&#xA;    End Sub&#xA;&#xA;For Each rawLine As String In arr&#xA;&#xA;    Dim line As String = &quot;&quot;&#xA;    If rawLine IsNot Nothing Then&#xA;        line = rawLine.Trim()&#xA;    End If&#xA;&#xA;    If String.IsNullOrWhiteSpace(line) Then&#xA;        Continue For&#xA;    End If&#xA;&#xA;    If rxIgnore.IsMatch(line) Then&#xA;        Continue For&#xA;    End If&#xA;&#xA;    ' SKU inline o al inicio; guardarlo sin espacios y en mayúsculas (80015 X2 -&gt; 80015X2)&#xA;    Dim mSku As System.Text.RegularExpressions.Match = rxSku.Match(line)&#xA;    If mSku.Success Then&#xA;        currentSku = mSku.Groups(1).Value.Trim().Replace(&quot; &quot;, &quot;&quot;).ToUpper()&#xA;        ' No hacemos Continue para permitir que esa misma línea (si trae más texto) NO se agregue al nombre.&#xA;        ' Aun así, como ya capturamos SKU, saltamos el manejo de nombre:&#xA;        Continue For&#xA;    End If&#xA;&#xA;    ' Precio: quitar separador de miles &quot;.&quot; (ej: 1.295,80 -&gt; 1295,80 ; 1.200 -&gt; 1200)&#xA;    Dim mPrice As System.Text.RegularExpressions.Match = rxPrice.Match(line)&#xA;    If mPrice.Success Then&#xA; 