                <ui:InvokeCode ContinueOnError="{x:Null}" Code="Try&#xA;    ' Reemplazar saltos de línea literales &quot;\n&quot; con Environment.NewLine&#xA;    text = text.Replace(&quot;\n&quot;, Environment.NewLine)&#xA;&#xA;    ' Dividir el texto en líneas&#xA;    Dim lineas As String() = text.Split(New String() {Environment.NewLine}, StringSplitOptions.None)&#xA;&#xA;    ' Encontrar el índice de la primera línea que no es de la venta (omitir la primera línea)&#xA;    Dim startIndex As Integer = 1 ' La primera línea (0) es la de la venta&#xA;    Dim endIndex As Integer = Array.FindIndex(lineas, Function(s) s.Contains(&quot;Seleccionar archivos&quot;))&#xA;    If endIndex = -1 Then&#xA;        endIndex = lineas.Length&#xA;    End If&#xA;&#xA;    ' Tomar las líneas de interés (artículos)&#xA;    Dim lineasArticulos As New List(Of String)()&#xA;    For i As Integer = startIndex To endIndex - 1&#xA;        lineasArticulos.Add(lineas(i))&#xA;    Next&#xA;&#xA;    ' Juntar las líneas de artículos en un string para dividir por &quot;u.&quot; + Environment.NewLine&#xA;    Dim textoArticulos As String = String.Join(Environment.NewLine, lineasArticulos)&#xA;    Dim separador As String = &quot;u.&quot; &amp; Environment.NewLine&#xA;    Dim bloques As String() = textoArticulos.Split(New String() {separador}, StringSplitOptions.RemoveEmptyEntries)&#xA;&#xA;    ' Crear un DataTable con las columnas requeridas&#xA;    dtVertical = New DataTable()&#xA;    dtVertical.Columns.Add(&quot;Text&quot;, GetType(String))          ' Columna A: Nombre del artículo&#xA;    dtVertical.Columns.Add(&quot;SKU&quot;, GetType(String))           ' Columna B: Código SKU&#xA;    dtVertical.Columns.Add(&quot;Precio&quot;, GetType(String))        ' Columna C: Precio&#xA;    dtVertical.Columns.Add(&quot;Cantidad&quot;, GetType(String))      ' Columna D: Cantidad&#xA;&#xA;    For Each bloque In bloques&#xA;        ' Dividir el bloque en líneas&#xA;        Dim lineasBloque As String() = bloque.Split(New String() {Environment.NewLine}, StringSplitOptions.RemoveEmptyEntries)&#xA;        &#xA;        ' Variables para los datos del artículo&#xA;        Dim nombre As String = &quot;&quot;&#xA;        Dim sku As String = &quot;&quot;&#xA;        Dim precio As String = &quot;&quot;&#xA;        Dim cantidad As String = &quot;1&quot; ' Por defecto, ya que cada bloque corresponde a un artículo con cantidad 1&#xA;        &#xA;        ' Buscar la línea que contiene &quot;SKU&quot;&#xA;        Dim lineaSKU As String = &quot;&quot;&#xA;        For Each linea In lineasBloque&#xA;            If linea.Contains(&quot;SKU&quot;) Then&#xA;                lineaSKU = linea&#xA;                Exit For&#xA;            End If&#xA;        Next&#xA;        &#xA;        ' Buscar la línea que contiene el precio (comienza con &quot;$&quot;)&#xA;        Dim lineaPrecio As String = &quot;&quot;&#xA;        For Each linea In lineasBloque&#xA;            If linea.Trim().StartsWith(&quot;$&quot;) Then&#xA;                lineaPrecio = linea&#xA;                Exit For&#xA;            End If&#xA;        Next&#xA;        &#xA;        ' Extraer el nombre: todas las líneas que no contengan &quot;SKU&quot; ni empiecen con &quot;$&quot;&#xA;        Dim nombreLineas As New List(Of String)()&#xA;        For Each linea In lineasBloque&#xA;            If Not linea.Contains(&quot;SKU&quot;) AndAlso Not linea.Trim().StartsWith(&quot;$&quot;) Then&#xA;                nombreLineas.Add(linea)&#xA;            End If&#xA;        Next&#xA;        nombre = String.Join(&quot; &quot;, nombreLineas).Trim()&#xA;        &#xA;        ' Extraer el SKU: primera palabra después de &quot;SKU&quot;&#xA;        If lineaSKU &lt;&gt; &quot;&quot; Then&#xA;            Dim indexSKU As Integer = lineaSKU.IndexOf(&quot;SKU&quot;)&#xA;            If indexSKU &gt;= 0 Then&#xA;                Dim restoSKU As String = lineaSKU.Substring(indexSKU + 3).Trim() ' 3 es la longitud de &quot;SKU&quot;&#xA;                Dim palabrasSKU As String() = restoSKU.Split(New Char() {&quot; &quot;c}, StringSplitOptions.RemoveEmptyEntries)&#xA;                If palabrasSKU.Length &gt; 0 Then&#xA;                    sku = palabrasSKU(0)&#xA;                Else&#xA;                    sku = restoSKU&#xA;                End If&#xA;            End If&#xA;        End If&#xA;        &#xA;        ' Extraer el precio: quitar &quot;$&quot;, espacios y puntos de miles&#xA;        If lineaPrecio &lt;&gt; &quot;&quot; Then&#xA;            precio = lineaPrecio.Replace(&quot;$&quot;, &quot;&quot;).Trim()&#xA;            precio = precio.Replace(&quot;.&quot;, &quot;&quot;).Trim() ' Quitar puntos de miles&#xA;        End If&#xA;        &#xA;        ' Agregar fila al DataTable&#xA;        dtVertical.Rows.Add(nombre, sku, precio, cantidad)&#xA;    Next&#xA;&#xA;    ' Mensaje de depuración (opcional, puedes eliminarlo después)&#xA;    Console.WriteLine(&quot;Se extrajeron &quot; &amp; dtVertical.Rows.Count &amp; &quot; artículos correctamente.&quot;)&#xA;    &#xA;Catch ex As Exception&#xA;    ' Manejo de errores&#xA;    Console.WriteLine(&quot;Error al procesar el texto: &quot; &amp; ex.Message)&#xA;    dtVertical = New DataTable() ' Retornar DataTable vacío en caso de error&#xA;End Try" DisplayName="Invocar código" sap:VirtualizedContainerService.HintSize="416,201" sap2010:WorkflowViewState.IdRef="InvokeCode_4">
